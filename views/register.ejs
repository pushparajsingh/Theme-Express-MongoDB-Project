<% include header %>

<% include nav %>

<% include slider %>


	<!-- page starts -->
	<div id="page">

		<!-- content starts -->
		<div id="content">
			<div class="box">
<h1>Register Here!!!</h1>
<font style="font-size:20px;" color="blue" ><%= output %></font>
<form method="POST" action="/register">
<table  cellspacing="10">
<tr>
<td>Name</td>
<td><input type="text" name="name" /></td>	
</tr>
<tr>
<td>Username</td>
<td><input type="text" name="username" /></td>	
</tr>	
<tr>
<td>Password</td>
<td><input type="password" name="password" /></td>	
</tr>
<tr>
<td>Mobile</td>
<td><input type="text" name="mobile" /></td>	
</tr>
<tr>
<td>Address</td>
<td><textarea rows="5" name="address"></textarea></td>	
</tr>
<tr>
<td>City</td>
<td>
	<select style="width:100%;" name="city">
		<option>Select City</option>
		<option>Indore</option>
		<option>Bhopal</option>
		<option>Ujjain</option>
	</select>
</td>	
</tr>
<tr>
<td>Gender</td>
<td>
Male <input type="radio" name="gender" value="male" />	
&nbsp;&nbsp;
Female <input type="radio" name="gender" value="female" />
</td>	
</tr>

<tr>
<td>
<input type="submit" value="Register" />	
</td>	
</tr>

</table>	
</form>

			</div>
			<br class="clearfix" />
		</div>
		<!-- content ends -->

		<!-- sidebar starts -->	
		<div id="sidebar">
			<div class="box">
				<h3>Social Links</h3>
				<ul class="list">
					<li class="first"><a href="#">@Facebook</a></li>
					<li><a href="#">@Twitter</a></li>
					<li><a href="#">@Instagram</a></li>
					<li><a href="#">@Gmail</a></li>
				</ul>
			</div>
		</div>
		<!-- sidebar ends -->

		<br class="clearfix" />
	</div>
	<!-- page ends -->

<% include footer %>