<% include header %>

<% include nav %>

<% include slider %>


	<!-- page starts -->
	<div id="page">

		<!-- content starts -->
		<div id="content">
			<div class="box">
				<h2>Welcome to eAuction , About Page</h2>
				
				<h1>Employee Details : Manual access</h1>
				<p>Eno : <%= empDetails['eno'] %></p>
				<p>Enm : <%= empDetails['enm'] %></p>
				<p>Esal : <%= empDetails.esal %></p>

				<h1>Marks list : Loop access</h1>
				<% for(let x of mks) { %>	
				<p><%= x %></p>
				<% } %>	
				<h1>Marks list : Manual access</h1>
				<p>marks1 : <%= mks[0] %></p>
				<p>marks2 : <%= mks[1] %></p>
				<p>marks3 : <%= mks[2] %></p>
				<p>marks4 : <%= mks[3] %></p>
				<p>marks5 : <%= mks[4] %></p>

				<h1>Add Result</h1>
				<p>a = <%= a %></p>
				<p>b = <%= b %></p>
				<p>add = <%= c %></p>
			</div>
			<br class="clearfix" />
		</div>
		<!-- content ends -->

		<!-- sidebar starts -->	
		<div id="sidebar">
			<div class="box">
				<h3>Social Links</h3>
				<ul class="list">
					<li class="first"><a href="#">@Facebook</a></li>
					<li><a href="#">@Twitter</a></li>
					<li><a href="#">@Instagram</a></li>
					<li><a href="#">@Gmail</a></li>
				</ul>
			</div>
		</div>
		<!-- sidebar ends -->

		<br class="clearfix" />
	</div>
	<!-- page ends -->

<% include footer %>